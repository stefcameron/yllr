//
// test jsHint settings.
// @see http://www.jshint.com/docs/options/
//

{
  "extends": "../.jshintrc",

  // Enforced options, producing *more* warnings

  // Relaxed options, producing *less* warnings

  // Environment options
  "browser": true,
  "node": false,
  "globals": {
    // AMD, CommonJS
    "require": true,

     // AMD
    "define": true,

    // CommonJS
    "module": true,
    "exports": true,

    // Lodash
    "_": true,

    // Jasmine
    "jasmine": true,
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "module": true,
    "browser": true,
    "describe": true,
    "xdescribe": true,
    "expect": true,
    "inject": true,
    "it": true,
    "xit": true,
    "spyOn": true
  }
}
